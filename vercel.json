{
    "version": 2,
    "builds": [
      {
        "src": "PetPalBackend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "PetPalFrontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "PetPalBackend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "PetPalFrontend/dist/$1"
      }
    ]
  }
  