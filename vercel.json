{
    "version": 2,
    "projects": [
      {
        "name": "frontend",
        "rootDirectory": "PetPalFrontend",
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      },
      {
        "name": "backend",
        "rootDirectory": "PetPalBackend"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "PetPalBackend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "PetPalFrontend/dist/$1"
      }
    ]
  }
  